{
  "name": "electron-vue3-quiet",
  "version": "0.0.0",
  "description": "桌面端脚手架",
  "author": "安静 <528627554@qq.com>",
  "license": "MIT",
  "homepage": "https://github.com/TaiAiAc/electron-vue3-quiet#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TaiAiAc/electron-vue3-quiet.git"
  },
  "bugs": {
    "url": "https://github.com/TaiAiAc/electron-vue3-quiet/issues"
  },
  "keywords": [
    "electron",
    "vue3",
    "typescript",
    "vite",
    "rollup",
    "electron-builder",
    "typeorm"
  ],
  "main": "./dist/main.js",
  "engines": {
    "node": ">=16"
  },
  "scripts": {
    "dev": "esno scripts/dev.ts",
    "build": "esno scripts/default-build.ts",
    "build:options": "esno scripts/options-build.ts",
    "clear:build": "rimraf dist && rimraf out",
    "typecheck": "vue-tsc --noEmit --skipLibCheck",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "postinstall": "electron-builder install-app-deps",
    "prepare": "husky install",
    "analysis": "windicss-analysis",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:c8": "vitest run --coverage",
    "dep:upgrade": "yarn upgrade-interactive --latest",
    "commit": "git-cz",
    "commit-push": "git pull && git add . && git-cz && git push"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    }
  },
  "dependencies": {
    "axios": "^0.27.2",
    "ffi-napi": "^4.0.3",
    "font-list": "^1.4.5",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.5.6",
    "sqlite3": "^5.0.11",
    "typeorm": "^0.3.7",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.27.0",
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-conventional": "^17.0.3",
    "@iconify/json": "^2.1.86",
    "@iconify/vue": "^3.2.1",
    "@rollup/plugin-alias": "^3.1.9",
    "@rollup/plugin-commonjs": "^22.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@rollup/plugin-replace": "^4.0.0",
    "@types/ffi-napi": "^4.0.5",
    "@types/inquirer": "^8.2.1",
    "@types/sqlite3": "^3.1.8",
    "@types/uuid": "^8.3.4",
    "@vitejs/plugin-vue": "^3.0.1",
    "@vitejs/plugin-vue-jsx": "^2.0.0",
    "@vue/compiler-sfc": "^3.2.37",
    "@vueuse/core": "^9.0.2",
    "c8": "^7.12.0",
    "colord": "^2.9.2",
    "commitizen": "^4.2.5",
    "cz-conventional-changelog": "^3.3.0",
    "cz-customizable": "^6.9.1",
    "dayjs": "^1.11.4",
    "dotenv": "^16.0.1",
    "electron": "^20.0.0",
    "electron-builder": "^23.3.3",
    "electron-devtools-vendor": "^1.1.0",
    "electron-log": "^4.4.7",
    "esbuild": "^0.14.53",
    "eslint": "^8.25.0",
    "esno": "^0.16.3",
    "follow-redirects": "^1.15.1",
    "fs-extra": "^10.1.0",
    "husky": "^8.0.1",
    "inquirer": "^9.0.2",
    "javascript-obfuscator": "^4.0.0",
    "lint-staged": "^13.0.1",
    "naive-ui": "^2.32.1",
    "pinia": "^2.0.17",
    "portfinder": "^1.0.28",
    "rimraf": "^3.0.2",
    "rollup": "^2.77.2",
    "rollup-plugin-esbuild": "^4.8.2",
    "rollup-plugin-obfuscator": "^0.2.2",
    "rollup-plugin-visualizer": "^5.7.1",
    "sass": "^1.54.1",
    "tslib": "^2.4.0",
    "typescript": "^4.7.3",
    "unplugin-auto-import": "^0.10.3",
    "unplugin-icons": "^0.14.8",
    "unplugin-vue-components": "0.21.2",
    "vfonts": "^0.0.3",
    "vite": "3.0.4",
    "vite-plugin-eslint": "^1.7.0",
    "vite-plugin-windicss": "^1.8.7",
    "vitest": "^0.20.3",
    "vue": "^3.2.37",
    "vue-i18n": "^9.2.0",
    "vue-router": "^4.1.3",
    "vue-tsc": "^0.39.4",
    "windicss": "^3.5.4",
    "windicss-analysis": "^0.3.5",
    "zx": "^7.0.8"
  }
}
